}#include <iostream>
using namespace std;
#define N 3
#define N1 9
int main()
{
    int i,j, a[N]={3,4,1,};
    
    for(i=0;i<N;i++){
        printf(" a[%d]= %d \n",i,a[i]);
    }
    
   for(i=1;i<N1;i++){
    for(j=1;j<N1;j++){
      printf("%2d * %2d = %2d  ",i,j,i*j);
    }
    printf(" \n");
    
    }
    cout <<"Hellow World";
       
    return 0;
